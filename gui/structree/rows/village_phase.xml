<?xml version="1.0" encoding="utf-8"?>

<object name="row_village_phase" size="8 16 100% 100%">
	<repeat count="11" var="s">
		
		<object type="image" style="StructBox" name="phase_village_struct[s]">
			<object type="text" style="StructNameSpecific" name="phase_village_struct[s]_name"/>
			<object type="image" style="StructIcon" name="phase_village_struct[s]_icon"
				sprite="stretched:pregame/shell/logo/wfg_logo_white.png"
			/>
			<repeat count="3" var="r">
				<object style="VillagePhaseProdRow" name="phase_village_struct[s]_row[r]">
					<repeat count="9" var="p">
						<object type="image" style="ProdBox" name="phase_village_struct[s]_row[r]_prod[p]"/>
					</repeat>
				</object>
			</repeat>
		</object>
		
	</repeat>
</object>